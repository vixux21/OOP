class Example {
  public static void main(String[] args) {
    try {
      int a = 5;
      int b = 0;
      int c = a / b;
      System.out.println(c);
    } catch (ArithmeticException e) {
      System.out.println("Cannot divide by zero.");
    } finally {
      System.out.println("This block will always execute.");
    }
  }
}
